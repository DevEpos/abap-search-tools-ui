<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.openindbbrowser"
            name="%command.openindbbrowser.name">
         <commandParameter
               id="com.devepos.adt.saat.openindbbrowser.skipSelscreenParam"
               name="skipSelscreen"
               optional="false"
               values="com.devepos.adt.saat.internal.handlers.OpenInDbBrowserParamValues">
         </commandParameter>
      </command>
      <category
            id="com.devepos.adt.saat"
            name="%category.name">
      </category>
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.opendbbrowser"
            name="%command.opendbbrowser.name">
      </command>
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.performCdsTopDown"
            name="%command.cdstopdown.name">
      </command>
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.performWhereUsedInCds"
            name="%command.whereusedincds.name">
      </command>
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.performCdsUsedEntities"
            name="%command.cdsusedentities.name">
      </command>
      <command
            categoryId="com.devepos.adt.saat"
            id="com.devepos.adt.saat.performFieldAnalysis"
            name="%command.fieldanalysis.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.devepos.adt.saat.internal.handlers.OpenInDbBrowserHandler"
            commandId="com.devepos.adt.saat.openindbbrowser">
         <enabledWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.devepos.adt.saat.internal.handlers.OpenDbBrowserHandler"
            commandId="com.devepos.adt.saat.opendbbrowser">
         <activeWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.devepos.adt.saat.internal.handlers.PerformCdsTopDownAnalysisHandler"
            commandId="com.devepos.adt.saat.performCdsTopDown">
         <enabledWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         
         </enabledWhen>
      </handler>
      <handler
            class="com.devepos.adt.saat.internal.handlers.PerformCdsUsedEntitiesAnalysisHandler"
            commandId="com.devepos.adt.saat.performCdsUsedEntities">
         <enabledWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.devepos.adt.saat.internal.handlers.PerformWhereUsedInCdsAnalysisHandler"
            commandId="com.devepos.adt.saat.performWhereUsedInCds">
         <enabledWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.devepos.adt.saat.internal.handlers.PerformFieldAnalysisHandler"
            commandId="com.devepos.adt.saat.performFieldAnalysis">
         <enabledWhen>
            <reference
                  definitionId="com.sap.adt.project.ui.adtActive">
            </reference>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:com.sap.adt.ris.search.ui.toolbar">
         <command
               commandId="com.devepos.adt.saat.opendbbrowser"
               icon="icons/DBBrowser.png"
               id="com.devepos.adt.saat.ui.toolBar"
               style="push"
               tooltip="Open DB Browser">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.sap.adt.project.ui.adtActive">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=com.sap.adt.datapreview.ui.opendatapreview.fromproject">
         <command
               commandId="com.devepos.adt.saat.opendbbrowser"
               icon="icons/DBBrowser.png"
               id="com.devepos.adt.saat.menus.openindbbrowser"
               label="%command.opendbbrowser.label"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <test
                        property="org.eclipse.core.resources.projectNature"
                        value="com.sap.adt.abapnature">
                  </test>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:#EditorContext?after=group.open">
         <dynamic
               class="com.devepos.adt.saat.internal.ui.DynamicOpenInMenu"
               id="com.devepos.adt.saat.editor.openInDynamicMenu">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditorInput">
                  <adapt
                        type="com.devepos.adt.saat.internal.util.IAdtObject">
                  </adapt>
               </with>
            </visibleWhen>
         </dynamic>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?endof=group.open">
         <dynamic
               class="com.devepos.adt.saat.internal.ui.DynamicOpenInMenu"
               id="com.devepos.adt.saat.projectExplorer.openInDynamicMenu">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <iterate
                        operator="and">
                     <adapt
                           type="com.devepos.adt.saat.internal.util.IAdtObject">
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </dynamic>
      </menuContribution>
   </extension>
     <extension
           point="org.eclipse.ui.preferencePages">
        <page
              class="com.devepos.adt.saat.internal.preferences.MainPreferencePage"
              id="com.devepos.adt.saat.ui.internal.MainPreferencePage"
              name="%preferencepage.name">
        </page>
     </extension>
     <extension
           point="org.eclipse.core.runtime.preferences">
        <initializer
              class="com.devepos.adt.saat.internal.preferences.PreferenceInitializer">
        </initializer>
     </extension>
     <extension
           point="org.eclipse.core.expressions.propertyTesters">
        <propertyTester
              class="com.devepos.adt.saat.internal.propertytester.AdtObjectPropertyTester"
              id="com.devepos.adt.saat.adtObjectPropertyTester"
              namespace="com.devepos.adt.saat.adtobject"
              properties="type"
              type="com.devepos.adt.saat.internal.util.IAdtObject">
        </propertyTester>
        <propertyTester
              class="com.devepos.adt.saat.internal.propertytester.CdsAnalysisPropertyTester"
              id="com.devepos.adt.saat.cdsAnalysisPropertyTester"
              namespace="com.devepos.adt.saat.cdsAnalysis"
              properties="isFeatureAvailable"
              type="com.devepos.adt.saat.internal.util.IAdtObject">
        </propertyTester>
     </extension>
     <extension
           point="org.eclipse.ui.views">
        <view
              category="com.sap.adt.views"
              class="com.devepos.adt.saat.internal.cdsanalysis.ui.CdsAnalysis"
              icon="icons/CdsAnalyzerView.png"
              id="com.devepos.adt.saat.views.cdsanalyzer"
              name="%view.cdsanalysis.name"
              restorable="true">
        </view>
     </extension>
     <extension
           point="org.eclipse.core.runtime.adapters">
        <factory
              adaptableType="java.lang.Object"
              class="com.devepos.adt.saat.internal.util.DataPreviewAdtObjectAdapterFactory">
           <adapter
                 type="com.devepos.adt.saat.internal.util.IAdtObject">
           </adapter>
        </factory>
     </extension>
     <extension
           point="org.eclipse.search.searchPages">
        <page
              class="com.devepos.adt.saat.internal.search.ui.ObjectSearchPage"
              icon="icons/ABAPSearch.png"
              id="com.devepos.adt.saat.ObjectSearchPage"
              label="%searchpage.objectsearch.name"
              tabPosition="1">
        </page>
     </extension>
     <extension
           point="org.eclipse.search.searchResultViewPages">
        <viewPage
              class="com.devepos.adt.saat.internal.search.ui.ObjectSearchResultPage"
              icon="icons/ABAPSearch.png"
              id="com.devepos.adt.saat.internal.search.ui.ObjectSearchResultPage"
              searchResultClass="com.devepos.adt.saat.internal.search.ui.ObjectSearchResult">
        </viewPage>
     </extension>
</plugin>
